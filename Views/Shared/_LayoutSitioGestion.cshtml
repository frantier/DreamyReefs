    <script type="text/javascript">
        // Cargar la biblioteca de Google Charts
        google.charts.load('current', { 'packages': ['corechart'] });

        // Callback para dibujar el gráfico cuando se cargue la biblioteca
        google.charts.setOnLoadCallback(drawChart);

        function drawChart() {
            // Obtener los datos pasados desde el controlador usando ViewBag
            var rawData = @Html.Raw(Json.Serialize(ViewBag.ChartData));

            // Convertir los datos de JSON a un array de arrays
            var dataArray = [['Tour', 'Total']]; // Encabezado de las columnas

            for (var i = 0; i < rawData.length; i++) {
                dataArray.push([rawData[i].tour, rawData[i].total]);
            }

            // Convertir el array de arrays en un objeto DataTable de Google Charts
            var data = google.visualization.arrayToDataTable(dataArray);

            // Aplicar formato de dinero a la columna "Total" en el DataTable
            var formatter = new google.visualization.NumberFormat({
                prefix: '$', // Símbolo "$" antes del valor
                fractionDigits: 0 // Dos decimales
            });
            formatter.format(data, 1);

            if (data.getNumberOfRows() === 0) {
                // Mostrar un mensaje personalizado en lugar del gráfico
                document.getElementById('chart_div').innerText = "No hay datos disponibles para mostrar el gráfico.";
                return;
            }

            // Configuración del gráfico de barras horizontales
            var options = {
                title: 'TOURS MAS SOLICITADOS',
                is3D: true,
                animation: {
                    duration: 1000,
                    easing: 'out',
                    startup: true
                },
                backgroundColor: 'whitesmoke',
                chartArea: { left: 150, right: 50, top: 80, bottom: 80 },
                annotations: {
                    textStyle: {
                        fontSize: 12,
                        bold: true,
                        color: 'black',
                        auraColor: 'none'
                    },
                },
                slices: {} // Dejamos inicialmente vacío, lo llenaremos más adelante
            };

            // Generar colores pastel y asignarlos a las barras
            var numRows = data.getNumberOfRows();
            for (var i = 0; i < numRows; i++) {
                options.slices[i] = { color: getRandomPastelColor() };
            }

            // Dibujar el gráfico de barras horizontales
            var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
            chart.draw(data, options);
        }

        // Función para generar colores pastel aleatorios
        function getRandomPastelColor() {
            var min = 100; // Valor mínimo para los componentes RGB
            var max = 230; // Valor máximo para los componentes RGB
            var r = Math.floor(Math.random() * (max - min + 1) + min);
            var g = Math.floor(Math.random() * (max - min + 1) + min);
            var b = Math.floor(Math.random() * (max - min + 1) + min);
            return 'rgb(' + r + ',' + g + ',' + b + ')';
        }
    </script>

