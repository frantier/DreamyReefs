                                location.reload(); // Actualiza la página después de eliminar el registro
                            });
                        },
                        error: function () {
                            swal.fire("Error", "Ha ocurrido un error al eliminar el registro.", "error");
                        }
                    });
                }
            })
        }

        function cerrarSesion() {
            Swal.fire({
                title: 'Cerrando sesión',
                html: 'Un momento por favor...',
                timer: 2000,
                timerProgressBar: true,
                didOpen: () => {
                    Swal.showLoading();

                    // Realizar la solicitud AJAX para cerrar sesión
                    $.ajax({
                        url: '@Url.Action("Salir", "Dashboard")',
                        type: 'POST',
                        error: function () {
                            // Mostrar un mensaje de error si ocurrió un problema durante el cierre de sesión
                            Swal.fire('Error', 'Ocurrió un error al cerrar sesión', 'error');
                        }
                    });
                },
                willClose: () => {
                    window.location.href = '/Login/Login'; // Redireccionar después de cerrar sesión una vez que el temporizador de Swal haya terminado
                }
            });
        }


    </script>

    <script>
        window.addEventListener('DOMContentLoaded', (event) => {
            const files = document.querySelectorAll('.fancy-file');
            Array.from(files).forEach(f => {
                const span = f.nextElementSibling.querySelector('span');

                // Muestra el nombre del archivo actual cuando se carga la página
                span.innerHTML = f.value || 'Ningún archivo seleccionado';

                f.addEventListener('change', e => {
                    // Actualiza el nombre del archivo cuando se selecciona un archivo nuevo
                    span.innerHTML = e.target.files[0]?.name || 'Ningún archivo seleccionado';
                });
            });
        });
    </script>


